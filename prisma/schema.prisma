generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Note {
  id          String  @id @default(cuid())
  title       String
  description String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
